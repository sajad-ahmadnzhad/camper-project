          <div class="circle-pulse__1"></div>
          <div class="circle-pulse__2"></div>
        </div>
        <div class="preloader__progress"><span></span></div>
      </div>
    </div>

    <main class="main">
      <!-- Header Image -->
      <div class="header-image">
        <div class="js-parallax" style="background-image: url('<%- ownerInfo && ownerInfo.mainCover%>')"></div>
      </div>

      <div class="container gutter-top">
        <!-- Header -->
        <%- include("./partials/_header-info.ejs") %>
        <div class="row sticky-parent">
          <!-- Sidebar nav -->
          <%-include("./partials/_side-menu.ejs")%>

          <!-- Content -->
          <%- body %>
